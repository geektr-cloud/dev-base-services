{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": ["s3:*"],
      "Resource": [
        "arn:aws:s3:::gitlab-lfs/*",
        "arn:aws:s3:::gitlab-uploads/*",
        "arn:aws:s3:::gitlab-artifacts/*",
        "arn:aws:s3:::gitlab-extdiffs/*",
        "arn:aws:s3:::gitlab-registry/*",
        "arn:aws:s3:::gitlab-runner/*"
      ]
    }
  ]
}
